{
  "name": "@2030/eslint-config",
  "type": "module",
  "version": "3.0.0",
  "description": "Shareable ESLint config for conventional coding style",
  "author": "Dai Jun <zijun2030@163.com>",
  "license": "MIT",
  "homepage": "https://github.com/Jun2030/eslint-config",
  "keywords": [
    "eslint-config"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./dist/cli.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": "./bin/index.mjs",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "nr gen && tsdown --clean --dts",
    "stub": "tsdown",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "build:inspector": "pnpm build && npx @eslint/config-inspector build",
    "watch": "tsdown --watch",
    "lint": "eslint",
    "gen": "tsx scripts/typegen.ts && tsx scripts/versiongen.ts",
    "prepack": "nr build",
    "release": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@eslint-react/eslint-plugin": "^2.0.1",
    "@next/eslint-plugin-next": ">=15.0.0",
